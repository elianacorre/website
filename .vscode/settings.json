{
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit",
    "source.fixAll.biome": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[graphql]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "emmet.showExpandedAbbreviation": "never",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#d08d7b",
    "activityBar.background": "#d08d7b",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#2a763a",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#d08d7b",
    "statusBar.background": "#c26c56",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#a8533d",
    "statusBarItem.remoteBackground": "#c26c56",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#c26c56",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#c26c5699",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#c26c56"
}
