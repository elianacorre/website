{
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[css]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.codeActionsOnSave": {
		"source.action.useSortedKeys.biome": "explicit",
		"source.removeUnusedImports": "explicit",
		"source.organizeImports.biome": "explicit"
	},
	"files.readonlyInclude": {
		"**/routeTree.gen.ts": true
	},
	"files.watcherExclude": {
		"**/routeTree.gen.ts": true
	},
	"peacock.color": "#5274ca",
	"search.exclude": {
		"**/routeTree.gen.ts": true
	},
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#7993d6",
		"activityBar.background": "#7993d6",
		"activityBar.foreground": "#15202b",
		"activityBar.inactiveForeground": "#15202b99",
		"activityBarBadge.background": "#ab3455",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#7993d6",
		"statusBar.background": "#5274ca",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#7993d6",
		"statusBarItem.remoteBackground": "#5274ca",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#5274ca",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#5274ca99",
		"titleBar.inactiveForeground": "#e7e7e799"
	}
}
